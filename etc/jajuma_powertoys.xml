<powertoys xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Jajuma_PowerToys:etc/jajuma_powertoys.xsd">
    <dashboard id="pot_rum" class="Jajuma\PotRum\Block\PowerToys\RumDashboard">
        <label translate="true">Rum Widget</label>
        <arguments>
            <argument name="enable">true</argument>
            <argument name="template">Jajuma_PotRum::widget/rum.phtml</argument>
            <argument name="lazyload">true</argument>
            <argument name="cache_lifetime">30</argument>
            <argument name="sort_order">50</argument>
            <argument name="acl_resource"></argument>
        </arguments>
    </dashboard>
</powertoys>
